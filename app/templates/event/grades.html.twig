{% extends 'base.html.twig' %}

{% block title %}
    {{ 'title.event_list'|trans }}
{% endblock %}

{% block body %}
    {% set sum = 0 %}
    {% set average = 0 %}
    {% if pagination|length %}
        {% for grade in pagination %}
            {% set sum = sum + grade.grade %}
        {% endfor %}
        {% set average = sum / pagination|length %}
        {{ average }}
    {% endif %}
{% endblock %}